{
  "name": "universal-react-redux-boilerplate",
  "version": "2.0.0",
  "description": "Step by step creation of a Universal React + Redux Boilerplate",
  "main": "index.js",
  "scripts": {
    "dev": "node --trace-warnings ./bin/start",
    "dev-debug": "node --trace-warnings --inspect --debug-brk ./bin/start",
    "start": "cross-env NODE_ENV=production node ./bin/start",
    "start-debug": "cross-env NODE_ENV=production node --inspect --debug-brk ./bin/start",
    "build": "npm run clean && npm run build:server && npm run build:client",
    "build:server": "cross-env NODE_ENV=production babel src --out-dir lib --copy-files",
    "build:client": "cross-env NODE_ENV=production webpack --display-modules --progress",
    "clean": "rimraf dist lib",
    "check": "npm run lint && npm run test",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --cache .",
    "lint:css": "stylelint src/**/*.scss --syntax scss",
    "test": "cross-env NODE_ENV=test mocha mocha.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com"
  },
  "keywords": [
    "isomorphic",
    "universal",
    "react",
    "redux",
    "boilerplate"
  ],
  "author": "anonymous",
  "license": "private",
  "dependencies": {
    "clean-css": "^4.1.4",
    "compression": "^1.6.2",
    "express": "^4.15.3",
    "lodash.debounce": "^4.0.8",
    "prop-types": "^15.5.10",
    "purify-css": "^1.2.5",
    "react": "^15.6.1",
    "react-children-utilities": "^0.1.12",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.1.3",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-config": "^1.0.0-beta.3",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "next",
    "redux": "^3.6.0",
    "redux-oauth": "^2.0.5",
    "redux-thunk": "^2.2.0",
    "serve-favicon": "^2.4.3",
    "superagent": "^3.5.2",
    "to-camel-case": "^1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.0.2",
    "chai-as-promised": "^7.0.0",
    "chai-enzyme": "^0.7.1",
    "chokidar": "^1.7.0",
    "compression-webpack-plugin": "^0.4.0",
    "console-loader": "^1.0.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "css-modules-require-hook": "^4.0.6",
    "cssnano": "^3.10.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
